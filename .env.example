# 应用配置
APP_NAME="智能穿搭分析Agent"
APP_VERSION="0.1.0"
APP_DESCRIPTION="基于Qwen-VL-Chat API和AutoGen的智能穿搭分析系统"
API_V1_STR="/api/v1"
SECRET_KEY="your-secret-key-here"
ACCESS_TOKEN_EXPIRE_MINUTES=10080

# CORS配置
BACKEND_CORS_ORIGINS=["*"]

# 数据库配置
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=fashion_scoring_agent
POSTGRES_PORT=5432
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/fashion_scoring_agent

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Qwen-VL-Chat API配置
QWEN_API_KEY=your-api-key-here
QWEN_API_BASE=https://dashscope.aliyuncs.com/api/v1
QWEN_MODEL_NAME=qwen-vl-chat

# 文件存储配置
UPLOAD_DIR=data/images
MAX_UPLOAD_SIZE=10485760
ALLOWED_IMAGE_TYPES=["image/jpeg", "image/png", "image/webp"]

# 日志配置
LOG_LEVEL=INFO